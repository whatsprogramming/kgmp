<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.31.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1757927498346-1" author="Tien Vu">
        <createTable tableName="student">
            <column name="id" type="VARCHAR(255)">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_student"/>
            </column>
            <column name="full_name" type="VARCHAR(255)"/>
            <column name="dob" type="DATETIME"/>
            <column name="gender" type="VARCHAR(255)"/>
            <column name="address" type="VARCHAR(255)"/>
            <column name="parent_name" type="VARCHAR(255)"/>
            <column name="parent_phone" type="VARCHAR(255)"/>
            <column name="status" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>